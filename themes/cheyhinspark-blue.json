	"tokenColors": [
		{
			"scope": "markup.bold",
			"settings": {
				"foreground": "#FDB975",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"foreground": "#FDB975",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#20A820"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#A82020"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#2020A8"
			}
		},
		
		{
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#FC3030"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#64A820",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"punctuation.section",
				"punctuation.separator",
				"punctuation.terminator",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#E0E0E0"
			}
		},

		{
			"scope": [
				"constant.character",
				"constant.numeric",
				"constant.other",
				"constant.language",
				"keyword.operator",
				"punctuation.math",
				"punctuation.definition.bold",
				"punctuation.separator.pointer-access"
			],
			"settings": {
				"foreground": "#FC9674"
			}
		},
		{
			"scope": [
				"entity.name.namespace",
				"entity.name.scope-resolution",
				"punctuation.definition.quote"
			],
			"settings": {
				"foreground": "#FCDA74"
			}
		},
		{
			"scope": [
				"meta.parameter",
				"variable.parameter",
				"entity.name.variable.parameter"
			],
			"settings": {
				"foreground": "#DAFC74",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"storage",
				"heading",
				"punctuation.definition.heading"
			],
			"settings": {
				"foreground": "#96FC74"
			}
		},
		{
			"scope": [
				"entity.name.section",
				"markup.heading"
			],
			"settings": {
				"foreground": "#96FC74",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"storage.type",
				"keyword.type",
				"keyword.other.type",
				"entity.name.type",
				"support.type"
			],
			"settings": {
				"foreground": "#74FC96"
			}
		},
		{
			"scope": [
				"variable",
				"source",
				"meta.function-call.arguments",
				"entity.name.tag",
				"punctuation.definition.tag",
				"storage.modifier.import",
				"markup.inline",
				"constant.character.math",
				"punctuation.definition.constant.math",
				"support.class.math"
			],
			"settings": {
				"foreground": "#74FCDA"
			}
		},
		{
			"scope": [
				"meta.attribute",
				"meta.member",
				"meta.object.member",
				"entity.other.attribute-name",
				"variable.other.member",
				"entity.name.variable.property",
				"meta.property-value",
				"variable.other.property",
				"variable.other.object.property",
				"variable.object.property",
				"constant.other.general.math"
			],
			"settings": {
				"foreground": "#74DAFC"
			}
		},
		{
			"scope": [
				"support.function",
				"entity.name.function",
				"meta.function-call.generic"
			],
			"settings": {
				"foreground": "#7496FC"
			}
		},
		{
			"scope": [
				"meta.preamble",
				"meta.body.class",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"foreground": "#9674FC"
			}
		},
		{
			"scope": [
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#9674FC",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"keyword.declaration"
			],
			"settings": {
				"foreground": "#DA74FC"
			}
		},
		{
			"scope": [
				"keyword.control",
				"keyword.other",
				"punctuation.definition.directive"
			],
			"settings": {
				"foreground": "#FC74DA"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#FC7496"
			}
		}
	]
