	"semanticHighlighting": true,
	"semanticTokenColors": {
		"constant": {
			"foreground": "#FCA78B"
		},
		"parameter": {
			"foreground": "#8BA7FC",
			"fontStyle": "italic"
		},
		"property": {
			"foreground": "#5DD4FC"
		},
		"selfParameter": {
			"foreground": "#5DD4FC"
		},
		"variable": {
			"foreground": "#5DD4FC"
		},
		"method": {
			"foreground": "#A78BFC"
		},
		"method.declaration": {
			"fontStyle": "bold"
		},
		"function": {
			"foreground": "#A78BFC"
		},
		"function.declaration": {
			"fontStyle": "bold"
		},
		"class": {
			"foreground": "#4DC425"
		},
		"class.declaration": {
			"fontStyle": "bold"
		},
		"module": {
			"foreground": "#4DC425"
		},
		"namespace": {
			"foreground": "#4DC425"
		},
		"templateType": {
			"foreground": "#4DC425"
		},
		"type": {
			"foreground": "#4DC425"
		},
		"newOperator": {
			"foreground": "#FC5D85"
		},
		"operatorOverload": {
			"foreground": "#E0E0E0"
		},
		"memberOperatorOverload": {
			"foreground": "#E0E0E0"
		}
	},
	"tokenColors": [
		{
			"scope": "markup.bold",
			"settings": {
				"foreground": "#FDB975",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"foreground": "#FDB975",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},

		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#20A820"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#A82020"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#2020A8"
			}
		},
		
		{
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#FC3030"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#949494",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"keyword.operator",
				"punctuation.accessor",
				"punctuation.definition",
				"punctuation.math",
				"punctuation.other",
				"punctuation.section",
				"punctuation.separator",
				"punctuation.terminator",
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#E0E0E0"
			}
		},

		{
			"scope": [
				"constant.character",
				"constant.language",
				"constant.numeric",
				"constant.other",
				"punctuation.definition.bold",
				"punctuation.separator.pointer-access"
			],
			"settings": {
				"foreground": "#FCA78B"
			}
		},
		{
			"scope": [
				"punctuation.definition.string",
				"string",
				"punctuation.definition.quote"
			],
			"settings": {
				"foreground": "#D5FC5D"
			}
		},
		{
			"scope": [
				"entity.name.section",
				"markup.heading",
				"heading",
				"punctuation.definition.heading"
			],
			"settings": {
				"foreground": "#85FC5D",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.other.caps",
				"entity.name.namespace",
				"entity.name.scope-resolution",
				"entity.name.type",
				"keyword.declaration",
				"keyword.struct"
			],
			"settings": {
				"foreground": "#4DC425"
			}
		},
		{
			"scope": [
				"entity.name.variable.parameter",
				"meta.parameter.initialization",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#8BA7FC",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"constant.character.math",
				"entity.name.tag",
				"entity.name.variable",
				"entity.other.attribute-name",
				"markup.inline",
				"meta.attribute",
				"meta.function-call.arguments",
				"meta.member",
				"meta.object.member",
				"meta.property-value",
				"punctuation.definition.constant.math",
				"punctuation.definition.tag",
				"storage.modifier.import",
				"support.class.math",
				"support.type.property-name",
				"variable"
			],
			"settings": {
				"foreground": "#5DD4FC"
			}
		},
		{
			"scope": [
				"constant.other.general.math",
				"entity.name.function",
				"keyword.operator.sizeof",
				"meta.function-call.generic",
				"support.function"
			],
			"settings": {
				"foreground": "#A78BFC"
			}
		},
		{
			"scope": [
				"keyword.control",
				"keyword.operator.delete",
				"keyword.operator.expression.new",
				"keyword.operator.new",
				"keyword.other",
				"keyword.other.type",
				"keyword.type",
				"punctuation.definition.directive",
				"storage.modifier",
				"storage.type.modifier",
				"storage.type"
			],
			"settings": {
				"foreground": "#FC8BA7"
			}
		}
	]
}