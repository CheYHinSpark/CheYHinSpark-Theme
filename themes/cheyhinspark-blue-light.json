	"semanticHighlighting": true,
	"semanticTokenColors": {
		"macro": {
			"foreground": "#595959"
		},
		"memberOperatorOverload": {
			"foreground": "#595959"
		},
		"operatorOverload": {
			"foreground": "#595959"
		},
		"constant": {
			"foreground": "#A08020"
		},
		"class": {
			"foreground": "#40A020"
		},
		"class.declaration": {
			"fontStyle": "bold"
		},
		"module": {
			"foreground": "#40A020"
		},
		"namespace": {
			"foreground": "#40A020"
		},
		"templateType": {
			"foreground": "#40A020"
		},
		"type": {
			"foreground": "#40A020"
		},
		"enum": {
			"foreground": "#20A040"
		},
		"event": {
			"foreground": "#20A040"
		},
		"interface": {
			"foreground": "#20A040"
		},
		"property": {
			"foreground": "#2080A0"
		},
		"selfParameter": {
			"foreground": "#2080A0"
		},
		"variable": {
			"foreground": "#2080A0"
		},
		"parameter": {
			"foreground": "#2850C8",
			"fontStyle": "italic"
		},
		"method": {
			"foreground": "#5028C8"
		},
		"method.declaration": {
			"fontStyle": "bold"
		},
		"function": {
			"foreground": "#5028C8"
		},
		"function.declaration": {
			"fontStyle": "bold"
		},
		"newOperator": {
			"foreground": "#A02040"
		}
	},
	"tokenColors": [
		{
			"scope": "markup.bold",
			"settings": {
				"foreground": "#FC9630",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"foreground": "#FC9630",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},

		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#20A820"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#A82020"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#2020A8"
			}
		},
		
		{
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#FC3030"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#949494",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"keyword.operator",
				"punctuation.accessor",
				"punctuation.definition",
				"punctuation.math",
				"punctuation.other",
				"punctuation.section",
				"punctuation.separator",
				"punctuation.terminator",
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#595959"
			}
		},

		{
			"scope": [
				"constant.character",
				"constant.language",
				"constant.numeric",
				"constant.other",
				"punctuation.definition.bold",
				"punctuation.separator.pointer-access"
			],
			"settings": {
				"foreground": "#A08020"
			}
		},
		{
			"scope": [
				"punctuation.definition.string",
				"string",
				"punctuation.definition.quote"
			],
			"settings": {
				"foreground": "#80A020"
			}
		},
		{
			"scope": [
				"entity.name.section",
				"markup.heading",
				"heading",
				"punctuation.definition.heading"
			],
			"settings": {
				"foreground": "#50C828",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.other.caps",
				"entity.name.namespace",
				"entity.name.scope-resolution",
				"entity.name.type",
				"keyword.declaration",
				"keyword.struct"
			],
			"settings": {
				"foreground": "#40A020"
			}
		},
		{
			"scope": [
				"entity.name.tag",
				"entity.name.variable.event"
			],
			"settings": {
				"foreground": "#20A040"
			}
		},
		{
			"scope": [
				"constant.character.math",
				"entity.name.variable",
				"entity.other.attribute-name",
				"markup.inline",
				"meta.attribute",
				"meta.function-call.arguments",
				"meta.member",
				"meta.object.member",
				"meta.property-value",
				"punctuation.definition.constant.math",
				"storage.modifier.import",
				"support.class.math",
				"support.type.property-name",
				"variable"
			],
			"settings": {
				"foreground": "#2080A0"
			}
		},
		{
			"scope": [
				"entity.name.variable.parameter",
				"meta.parameter.initialization",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#2850C8",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"constant.other.general.math",
				"entity.name.function",
				"keyword.operator.sizeof",
				"meta.function-call.generic",
				"support.function"
			],
			"settings": {
				"foreground": "#5028C8"
			}
		},
		{
			"scope": [
				"keyword.control",
				"keyword.operator.delete",
				"keyword.operator.expression.new",
				"keyword.operator.new",
				"keyword.other",
				"keyword.other.type",
				"keyword.type",
				"punctuation.definition.directive",
				"storage.modifier",
				"storage.type.modifier",
				"storage.type"
			],
			"settings": {
				"foreground": "#A02040"
			}
		}
	]
}