		"interface": {
			"foreground": "#28C8A0"
		},
		"property": {
			"foreground": "#32C8FA"
		},
		"selfParameter": {
			"foreground": "#32C8FA"
		},
		"variable": {
			"foreground": "#32C8FA"
		},
		"parameter": {
			"foreground": "#7A9AFA",
			"fontStyle": "italic"
		},
		"method": {
			"foreground": "#9A7AFA"
		},
		"method.declaration": {
			"fontStyle": "bold"
		},
		"function": {
			"foreground": "#9A7AFA"
		},
		"function.declaration": {
			"fontStyle": "bold"
		},
		"newOperator": {
			"foreground": "#FA7A9A"
		}
	},
	"tokenColors": [
		{
			"scope": "markup.bold",
			"settings": {
				"foreground": "#FDB975",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"foreground": "#FDB975",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},

		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#20A820"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#A82020"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#2020A8"
			}
		},
		
		{
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#FC3030"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#949494",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"keyword.operator",
				"punctuation.accessor",
				"punctuation.definition",
				"punctuation.math",
				"punctuation.other",
				"punctuation.section",
				"punctuation.separator",
				"punctuation.terminator",
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#E0E0E0"
			}
		},

		{
			"scope": [
				"constant.character",
				"constant.language",
				"constant.numeric",
				"constant.other",
				"punctuation.definition.bold",
				"punctuation.separator.pointer-access"
			],
			"settings": {
				"foreground": "#FAC832"
			}
		},
		{
			"scope": [
				"punctuation.definition.string",
				"string",
				"punctuation.definition.quote"
			],
			"settings": {
				"foreground": "#C8FA32"
			}
		},
		{
			"scope": [
				"entity.name.section",
				"markup.heading",
				"heading",
				"punctuation.definition.heading"
			],
			"settings": {
				"foreground": "#64FA32",
				"fontStyle": "bold"
			}
		},
