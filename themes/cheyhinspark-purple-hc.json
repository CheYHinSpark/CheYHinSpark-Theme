	"semanticHighlighting": true,
	"semanticTokenColors": {
		"macro": {
			"foreground": "#E0E0E0"
		},
		"memberOperatorOverload": {
			"foreground": "#E0E0E0"
		},
		"operatorOverload": {
			"foreground": "#E0E0E0"
		},
		"constant": {
			"foreground": "#FAC832"
		},
		"class": {
			"foreground": "#50C828"
		},
		"class.declaration": {
			"fontStyle": "bold"
		},
		"module": {
			"foreground": "#50C828"
		},
		"namespace": {
			"foreground": "#50C828"
		},
		"templateType": {
			"foreground": "#50C828"
		},
		"type": {
			"foreground": "#50C828"
		},
		"enum": {
			"foreground": "#28C8A0"
