				"entity.other.attribute-name",
				"markup.inline",
				"meta.attribute",
				"meta.function-call.arguments",
				"meta.member",
				"meta.object.member",
				"meta.property-value",
				"punctuation.definition.constant.math",
				"storage.modifier.import",
				"support.class.math",
				"support.type.property-name",
				"variable"
			],
			"settings": {
				"foreground": "#32C8FA"
			}
		},
		{
			"scope": [
				"entity.name.variable.parameter",
				"meta.parameter.initialization",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#7A9AFA",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"constant.other.general.math",
