			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#2020A8"
			}
		},
		
		{
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#FC3030"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
