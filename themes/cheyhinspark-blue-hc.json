	"tokenColors": [
		{
			"scope": "markup.bold",
			"settings": {
				"foreground": "#FDB975",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"foreground": "#FDB975",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},

		{
			"scope": "markup.inserted",
			"settings": {
